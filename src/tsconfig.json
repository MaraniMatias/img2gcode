{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "inlineSources": false,
    "newLine": "LF",
    "help": true,
    "charset": "utf8",
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "preserveConstEnums": true
  }
}
