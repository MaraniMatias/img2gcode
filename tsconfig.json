{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "allowJs": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources":true,
    "newLine":"LF",
    "help":true,
    "charset":"utf8",
    "emitDecoratorMetadata": true,
    "removeComments": true,
    "noImplicitAny": false,
    "preserveConstEnums": true
  }
}